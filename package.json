{
  "name": "app-registry",
  "version": "1.0.0",
  "description": "Onos App Registry",
  "main": "./build/deploy.js",
  "scripts": {
    "clear": "rm -rf build/* ",
    "build:ts": "tsc --project tsconfig.json --watch false",
    "build:json": "node build/scripts/buildjson.js",
    "build": "npm run clear && npm run build:ts && npm run copy && npm run build:json",
    "test": "npm run build && mocha build/tests --exit",
    "start": "npm run build && node .",
    "watch": "tsc --project tsconfig.json --watch true",
    "dev": "nodemon .",
    "copy": "cp -a src/apps build/apps && cp -a src/views build/views"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onosproject/app-registry.git"
  },
  "author": "Open Networking Foundation",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/onosproject/app-registry/issues"
  },
  "homepage": "https://github.com/onosproject/app-registry#readme",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.133",
    "@types/mongoose": "^5.5.3",
    "@types/morgan": "^1.7.35",
    "@types/request-promise": "^4.1.44",
    "@types/shelljs": "^0.8.5",
    "@types/signale": "^1.2.1",
    "@types/yargs": "^13.0.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.13",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "request-promise": "^4.2.4",
    "shelljs": "^0.8.3",
    "signale": "^1.4.0",
    "typescript": "^3.5.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0"
  }
}
